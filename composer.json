{
    "name": "alibabacloud/sdk",
    "homepage": "https://www.alibabacloud.com/",
    "description": "Alibaba Cloud SDK for PHP - Easier to Use Alibaba Cloud in your PHP project",
    "keywords": [
        "aliyun",
        "alibaba",
        "alibabacloud",
        "sdk",
        "ecs",
        "cloud"
    ],
    "type": "library",
    "license": "Apache-2.0",
    "support": {
        "issues": "https://github.com/aliyun/openapi-sdk-php/issues",
        "source": "https://github.com/aliyun/openapi-sdk-php"
    },
    "authors": [
        {
            "name": "Alibaba Cloud SDK",
            "homepage": "http://www.alibabacloud.com",
            "email": "sdk-team@alibabacloud.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-json": "*",
        "ext-mbstring": "*",
        "alibabacloud/client": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.2",
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/dotenv": "^3.4",
        "symfony/var-dumper": "^3.4"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "AlibabaCloud\\": "src"
        },
        "files": [
            "src/Constants.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AlibabaCloud\\Tests\\": "tests/"
        },
        "classmap": [
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
